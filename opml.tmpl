<?xml version="1.0" encoding="UTF-8"?>
<!-- OPML generated by youtube-subscription-feed -->
<opml version="1.1">
    <head>
        <title>YouTube-Subscription-feed.opml</title>
    </head>
    <body>
        <outline text="🍿 YouTube" title="🍿 YouTube">
        {{ range . }}
            {{- with .Snippet -}}
                {{- $title := .Title -}}
                {{- $channelID := .ResourceId.ChannelId -}}
                <outline text="{{ html $title }}" title="{{ html $title }}" description="" type="rss" version="RSS" htmlUrl="https://www.youtube.com/channel/{{ html $channelID }}" xmlUrl="https://www.youtube.com/feeds/videos.xml?channel_id={{ html $channelID }}" />
            {{- end }}
        {{ end -}}
        </outline>
    </body>
</opml>